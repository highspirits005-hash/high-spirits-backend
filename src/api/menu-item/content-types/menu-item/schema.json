{
  "kind": "collectionType",
  "collectionName": "menu_items",
  "info": {
    "singularName": "menu-item",
    "pluralName": "menu-items",
    "displayName": "menuItem"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "shortDescription": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "price": {
      "type": "decimal"
    },
    "priceLabel": {
      "type": "string"
    },
    "featured": {
      "type": "boolean"
    },
    "displayOrder": {
      "type": "integer"
    },
    "menu_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::menu-category.menu-category",
      "inversedBy": "menu_items"
    }
  }
}
